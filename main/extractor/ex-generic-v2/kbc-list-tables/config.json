{
  "parameters": {
    "api": {
      "baseUrl": "https://connection.keboola.com/v2/storage/",
      "http": {
        "headers": {
          "Accept": "application/json",
          "X-StorageApi-Token": {
            "attr": "#storage_token"
          }
        }
      }
    },
    "config": {
      "debug": true,
      "outputBucket": "generic-ex-storage-mapping",
      "#storage_token": "KBC::ProjectSecure::",
      "jobs": [
        {
          "endpoint": "tables",
          "dataField": ".",
          "dataType": "storage_tables",
          "params": {
            "include": "buckets,columns"
          }
        }
      ],
      "mappings": {
        "storage_tables": {
          "uri": {
            "mapping": {
              "destination": "table_uri",
              "primaryKey": true
            }
          },
          "columns": {
            "type": "table",
            "destination": "table_columns",
            "parentKey": {
              "primaryKey": true,
              "destination": "table_uri"
            },
            "tableMapping": {
              ".": {
                "type": "column",
                "mapping": {
                  "destination": "name",
                  "primaryKey": true
                }
              }
            }
          },
          "id": "kbc_table_id",
          "name": "name",
          "created": "created",
          "lastImportDate": "last_import_date",
          "lastChangeDate": "last_change_date"
        }
      }
    }
  }
}
