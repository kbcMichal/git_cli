{
  "parameters": {
    "bucket": "",
    "key": "",
    "includeSubfolders": false,
    "newFilesOnly": false
  },
  "processors": {
    "after": [
      {
        "definition": {
          "component": "keboola.processor-move-files"
        },
        "parameters": {
          "direction": "tables",
          "addCsvSuffix": true,
          "folder": "test"
        }
      },
      {
        "definition": {
          "component": "keboola.processor-iconv"
        },
        "parameters": {
          "source_encoding": "WINDOWS-1250"
        }
      },
      {
        "definition": {
          "component": "keboola.processor-create-manifest"
        },
        "parameters": {
          "delimiter": ",",
          "enclosure": "\"",
          "incremental": false,
          "primary_key": [],
          "columns_from": "header"
        }
      },
      {
        "definition": {
          "component": "keboola.processor-skip-lines"
        },
        "parameters": {
          "lines": 1
        }
      }
    ]
  }
}
