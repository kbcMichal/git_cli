{
  "parameters": {
    "path": "/cd_catalog.xml"
  },
  "processors": {
    "after": [
      {
        "definition": {
          "component": "keboola.processor-fit-into-storage"
        },
        "parameters": {
          "chunk_size": 900000
        }
      },
      {
        "definition": {
          "component": "keboola.processor-move-files"
        },
        "parameters": {
          "direction": "tables",
          "folder": "cd_catalog"
        }
      },
      {
        "definition": {
          "component": "keboola.processor-create-manifest"
        },
        "parameters": {
          "delimiter": ",",
          "enclosure": "\"",
          "incremental": false,
          "primary_key": [],
          "columns_from": "header"
        }
      },
      {
        "definition": {
          "component": "keboola.processor-skip-lines"
        },
        "parameters": {
          "lines": 1
        }
      }
    ]
  }
}
