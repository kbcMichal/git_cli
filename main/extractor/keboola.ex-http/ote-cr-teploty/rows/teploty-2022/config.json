{
  "parameters": {
    "path": "Teploty_plyn_2022_CZ.zip"
  },
  "processors": {
    "after": [
      {
        "definition": {
          "component": "keboola.processor-decompress"
        }
      },
      {
        "definition": {
          "component": "jakub-bartel.processor-xls2csv"
        }
      },
      {
        "definition": {
          "component": "keboola.processor-skip-lines"
        },
        "parameters": {
          "lines": 5,
          "direction_from": "top"
        }
      },
      {
        "definition": {
          "component": "keboola.processor-move-files"
        },
        "parameters": {
          "direction": "tables",
          "folder": "teploty-2022"
        }
      },
      {
        "definition": {
          "component": "keboola.processor-flatten-folders"
        },
        "parameters": {
          "starting_depth": 1
        }
      },
      {
        "definition": {
          "component": "keboola.processor-create-manifest"
        },
        "parameters": {
          "delimiter": ",",
          "enclosure": "\"",
          "incremental": false,
          "primary_key": [],
          "columns": [
            "plynarensky_den",
            "den_v_roce",
            "cr_normalova_t",
            "cr_skutecna_t",
            "zapad_cechy_skutecna_t",
            "vychod_cechy_skutecna_t",
            "stred_cechy_skutecna_t",
            "sever_morava_skutecna_t",
            "sever_cechy_skutecna_t",
            "praha_skutecna_t",
            "jih_morava_skutecna_t",
            "jih_cechy_skutecna_t"
          ]
        }
      },
      {
        "definition": {
          "component": "keboola.processor-skip-lines"
        },
        "parameters": {
          "lines": 1
        }
      }
    ]
  }
}
